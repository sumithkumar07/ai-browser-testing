{
  "name": "kairo-browser",
  "version": "1.0.0",
  "description": "Kairo - AI-Powered Browser",
  "main": "electron/main/main.js",
  "scripts": {
    "dev": "npm run dev:vite & npm run dev:electron",
    "dev:vite": "vite",
    "dev:electron": "timeout 5 && set NODE_ENV=development && electron .",
    "build": "vite build",
    "build:electron": "npm run build && electron-builder",
    "build:web": "vite build",
    "preview": "vite preview",
    "start": "electron .",
    "package": "npm run build && electron-builder --publish=never"
  },
  "keywords": [
    "browser",
    "ai",
    "electron",
    "react"
  ],
  "author": "Kairo Team",
  "license": "MIT",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.2",
    "electron": "^27.0.0",
    "electron-builder": "^24.0.0",
    "electron-reload": "^1.5.0",
    "terser": "^5.44.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "groq-sdk": "^0.31.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "zustand": "^4.5.7"
  },
  "build": {
    "appId": "com.kairo.browser",
    "productName": "Kairo Browser",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
